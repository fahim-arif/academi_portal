{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf build && tsc --skipLibCheck",
    "coverage": "jest --coverage --runInBand",
    "db:setup": "ts-node ./src/test-utils/setup.ts",
    "db:teardown": "ts-node ./src/test-utils/teardown.ts",
    "dev": "nodemon -w src --ext ts --exec ts-node src/index.ts",
    "dev:debug": "nodemon --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register src/index.ts'",
    "migration:create": "yarn build && yarn typeorm:cli-dev migration:create -n",
    "migration:generate": "yarn build && yarn typeorm:cli-dev migration:generate -n",
    "migration:run": "yarn build && yarn typeorm:cli-dev migration:run",
    "migration:revert": "yarn build && yarn typeorm:cli-dev migration:revert",
    "postinstall": "yarn build",
    "redis:start": "redis-server /usr/local/etc/redis.conf",
    "start": "node build/index.js",
    "test": "jest --runInBand",
    "test:debug": "jest --silent=false --coverage=false --runInBand",
    "test-ci": "node build/test-utils/setup.js && jest",
    "watch-node": "nodemon build/index.js",
    "watch-ts": "tsc -w",
    "typeorm:cli": "node ./node_modules/typeorm/cli -f build/db/ormConfig.js",
    "typeorm:cli-dev": "ts-node ./node_modules/typeorm/cli -f ./src/db/ormConfig.ts",
    "command": "node ./build/tasks/commander.js",
    "command:js": "node ./build/tasks/commander.js"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@aws-sdk/client-s3": "^3.38.0",
    "@logtail/node": "^0.1.14",
    "@mux/mux-node": "^7.0.0",
    "@sendgrid/mail": "^7.5.0",
    "@sentry/node": "^7.17.4",
    "@thirdweb-dev/sdk": "^3.6.7",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "algoliasearch": "^4.11.0",
    "aws-sdk": "^2.1013.0",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "bullmq": "^3.5.1",
    "bunyan": "^1.8.15",
    "casual": "^1.6.2",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.27.1",
    "commander": "^9.4.1",
    "cors": "^2.8.5",
    "count-lines-in-file": "^1.1.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.17.1",
    "google-auth-library": "^7.10.1",
    "graphql": "^15.6.1",
    "graphql-subscriptions": "^2.0.0",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^8.5.1",
    "line-by-line": "^0.1.6",
    "mockdate": "^3.0.5",
    "moment": "^2.29.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "slugify": "^1.6.1",
    "throng": "^5.0.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.38",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@pollyjs/adapter-node-http": "^6.0.5",
    "@pollyjs/core": "^6.0.5",
    "@pollyjs/persister-fs": "^6.0.5",
    "@types/algoliasearch": "^4.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/faker": "5.5.9",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/line-by-line": "^0.1.5",
    "@types/node": "^16.11.4",
    "@types/request": "^2.48.7",
    "@types/throng": "^5.0.3",
    "@types/uuid": "^7.0.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "faker": "^5.4.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.4.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
